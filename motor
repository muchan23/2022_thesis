#電源が入ったと同時にモーターが一定のスピードで正転する時のプログラム

int AIN1 = 5;
int AIN2 = 6;
int BIN1 = 9;
int BIN2 = 10;

void setup()
{
    pinMode(AIN1,OUTPUT);
    pinMode(BIN1,OUTPUT);
}

void loop()
{
    digitalWrite(AIN1,HIGH);
    analogWrite(AIN2,125);  //PWM
    digitalWrite(BIN1,HIGH);
    analogWrite(BIN2,125);  //PWM
}